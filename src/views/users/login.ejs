<!DOCTYPE html>
<html>
  <head>
    <title>login</title>
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css">
    <link rel="stylesheet" type="text/css" href="/style.css">
    <script type="text/javascript" src="/script.js"></script>
  </head>
  <body>
    <div class="page-column">
    <%- include ../partials/nav.ejs %>
      <div class="page-column-content">
  <h1>Login Page</h1>
  <form method="post" action="/users/login" class="existing-user-form">
    <pre class="existing-user-form-errors"></pre>
    <label>
      <span>Email</span>
      <input type="text" name="email" />
    </label>
    <label>
      <span>Password</span>
      <input type="text" name="password" />
    </label>
    <input type="submit" value="Login">
  </form>
  <div class="signup-redirect">If you are not a member<a href="/users/signup"><button class="signup">Signup</button></a>
  </div>
  <h1><%- errorMsg %></h1>
<%- include ../partials/footer.ejs %>